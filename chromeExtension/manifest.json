{
  "manifest_version": 2,
  "name": "Legit Extension",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["javascript/jquery-3.1.1.min.js", "javascript/content.js"]
    }
  ],

  "background": {
  "scripts": ["javascript/background.js"]
  },

  "browser_action": {
  "default_icon": "icon.png"
  }
}
