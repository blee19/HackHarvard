{
  "manifest_version": 2,
  "name": "Legit Extension",
  "version": "0.1",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": [
    "history",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["javascript/content.js","javascript/jquery-3.1.1.min.js"]
    }
  ],

  "background": {
  "scripts": ["javascript/background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  }
}
